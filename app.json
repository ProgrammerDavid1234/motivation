{
  "expo": {
    "name": "All About You",
    "slug": "motivation-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/she.jpg",
    "scheme": "motivationapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/she.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.olonadenifemi.love"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/she.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/she.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "99f3782c-2299-4ffb-8777-ebf671f5209a"
      }
    },
    "owner": "olonadenifemi"
  }
}
